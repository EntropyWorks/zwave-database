            <Type>short</Type>
            <Default>28</Default>
            <Minimum>5</Minimum>
            <Maximum>60</Maximum>
            <Size>1</Size>
        </Parameter>

        <Parameter>
            <Index>52</Index>
            <Label lang="en">Pause after pulse</Label>
            <Type>short</Type>
            <Default>28</Default>
            <Minimum>5</Minimum>
            <Maximum>60</Maximum>
            <Size>1</Size>
        </Parameter>

        <Parameter>
            <Index>53</Index>
            <Label lang="en">Pulse width</Label>
            <Type>short</Type>
            <Default>10</Default>
            <Minimum>3</Minimum>
            <Maximum>20</Maximum>
            <Size>1</Size>
        </Parameter>

        <Parameter>
            <Index>54</Index>
            <Label lang="en">Pulse type</Label>
            <Type>list</Type>
            <Default>0</Default>
            <Minimum>0</Minimum>
            <Maximum>1</Maximum>
            <Size>1</Size>
            <Item>
                <Value>0</Value>
                <Label lang="en">Long</Label>
            </Item>
            <Item>
                <Value>1</Value>
                <Label lang="en">Short</Label>
            </Item>
        </Parameter>

