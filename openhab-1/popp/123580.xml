    <Configuration>

        <Parameter>
            <Index>1</Index>
            <Label lang="en">Set LED indication mode</Label>
            <Type>list</Type>
            <Default>1</Default>
            <Minimum>0</Minimum>
            <Maximum>3</Maximum>
            <Size>1</Size>
            <Item>
                <Value>0</Value>
                <Label lang="en">Disabled</Label>
            </Item>
            <Item>
                <Value>1</Value>
                <Label lang="en">On when switch on</Label>
            </Item>
            <Item>
                <Value>2</Value>
                <Label lang="en">On when switch off</Label>
            </Item>
            <Item>
                <Value>3</Value>
                <Label lang="en">Operated by command</Label>
            </Item>
        </Parameter>

        <Parameter>
            <Index>2</Index>
            <Label lang="en">Automatic switch off time</Label>
            <Type>short</Type>
            <Default>0</Default>
            <Minimum>0</Minimum>
            <Maximum>65535</Maximum>
            <Size>2</Size>
        </Parameter>

        <Parameter>
            <Index>3</Index>
            <Label lang="en">RF Off command</Label>
            <Type>list</Type>
            <Default>0</Default>
            <Minimum>0</Minimum>
            <Maximum>3</Maximum>
            <Size>1</Size>
            <Item>
                <Value>0</Value>
                <Label lang="en">Switch OFF</Label>
            </Item>
            <Item>
                <Value>1</Value>
                <Label lang="en">Ignore</Label>
            </Item>
            <Item>
                <Value>2</Value>
                <Label lang="en">Switch ON</Label>
            </Item>
            <Item>
                <Value>3</Value>
                <Label lang="en">Switch on if load is off else switch off</Label>
            </Item>
        </Parameter>

        <Parameter>
            <Index>4</Index>
            <Label lang="en">Ignore start level</Label>
            <Type>list</Type>
            <Default>1</Default>
            <Minimum>0</Minimum>
            <Maximum>1</Maximum>
            <Size>1</Size>
            <Item>
                <Value>0</Value>
                <Label lang="en">No</Label>
            </Item>
            <Item>
                <Value>1</Value>
                <Label lang="en">Yes</Label>
            </Item>
        </Parameter>

        <Parameter>
            <Index>5</Index>
            <Label lang="en">Speed for fast dimming</Label>
            <Type>short</Type>
            <Default>30</Default>
            <Minimum>0</Minimum>
            <Maximum>255</Maximum>
            <Size>1</Size>
        </Parameter>

        <Parameter>
            <Index>6</Index>
            <Label lang="en">Speed for slow dimming</Label>
            <Type>short</Type>
            <Default>3</Default>
            <Minimum>0</Minimum>
            <Maximum>255</Maximum>
            <Size>1</Size>
        </Parameter>

        <Parameter>
            <Index>7</Index>
            <Label lang="en">Behavior when ON received</Label>
            <Type>list</Type>
            <Default>0</Default>
            <Minimum>0</Minimum>
            <Maximum>2</Maximum>
            <Size>1</Size>
            <Item>
                <Value>0</Value>
                <Label lang="en">Set to last dim level</Label>
            </Item>
            <Item>
                <Value>1</Value>
                <Label lang="en">If on already, set to maximum</Label>
            </Item>
            <Item>
                <Value>2</Value>
                <Label lang="en">Always maximum dim level</Label>
            </Item>
        </Parameter>

        <Parameter>
            <Index>8</Index>
